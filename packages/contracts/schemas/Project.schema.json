{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://corebridge.dev/schemas/Project",
  "title": "Project",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "id",
    "createdAt",
    "updatedAt"
  ]
}
